include ../../Make.inc


Build: Basis \
	

	@cp -r ^Conf/*  $(Out)/Conf



Basis:
	@rm -rf $(Out)

	@mkdir -p $(Out)/Moq
	@mkdir -p $(Out)/Log
	@mkdir -p $(Out)/Conf




Clean:
	@rm -rf $(Out)

